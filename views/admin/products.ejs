                        <div class="bg-secondary rounded h-100 p-4">
                            <h6 class="mb-4">Prodicts Table</h6>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col">NO</th>
                                            <th scope="col">Name</th>
                                            <th scope="col">Amount</th>
                                            <th scope="col">Min Purchase</th>
                                            <th scope="col">Expiry Date</th>
                                            <th scope="col">Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% productList.forEach(function(ProductList,){ %> 
                                        <tr>
                                            <th scope="row"><%=1 %>  </th>
                                            <td><%=couponList.name %> </td>
                                            <td><%=couponList.amount %> </td>
                                            <td><%=couponList.minCartAmount%> </td>
                                            <td><%=couponList.expireDate%> </td>
                                            <td>
                                                <a href="/admin/couponEdit/<%=couponList._id %> " 
                                                    class="btn btn-success"
                                                     onclick = "return confirm('Are you want edit <%=  couponList.name%>')">edit</a>

                                                <% if (couponList.access) {%> 
                                                <a href="/admin/couponDisable/<%=couponList._id %> " 
                                                    class="btn btn-danger"
                                                     onclick = "return confirm('Are you want disable <%=  couponList.name%>')">Disable</a>
                                                <% }  else { %> 
                                                    <a href="/admin/couponEnable/<%=couponList._id %> " 
                                                        class="btn btn-danger" 
                                                        onclick = "return confirm('Are you want enable <%= couponList.name%>')">Enable</a>
                                                    <% } %> 
                                            </td>
                                        </tr>
                                        <% }) %> 
                                    </tbody>
                                </table>
                            </div>
                        </div>
